{
    "extends": "airbnb-base",
    "env": {
      "browser": true,
      "node": true
    },
    "rules": {
        "comma-dangle": ["error", "never"],
        "no-use-before-define": "off",
        "arrow-body-style": "off",
        "arrow-parens": ["error", "always"],
        "class-methods-use-this": "off",
        "object-curly-newline": "off"
    }
}
